<Window x:Class="Курсач_сайко_1125.TheBlessedOnes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Курсач_сайко_1125"
        mc:Ignorable="d"
        Title="Райские угодья" Height="450" Width="800">
    <Grid>
        <DataGrid ItemsSource="{Binding Yeszap}" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Имя" Binding="{Binding Name}" />
                <DataGridTextColumn Header="Средний балл" Binding="{Binding Gpa}" />
                <DataGridTextColumn Header="Специальность" Binding="{Binding Spec}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>